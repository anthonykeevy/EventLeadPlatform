    "@dnd-kit/sortable": "7.0.2",
    "@dnd-kit/utilities": "3.2.1",
    "@tanstack/react-query": "5.8.4",
    "axios": "1.6.2",
    "recharts": "2.10.1",
    "date-fns": "2.30.0",
    "lucide-react": "0.294.0",
    "@radix-ui/react-dialog": "1.0.5",
